      <TextField fx:id="passwordTextField" layoutX="45.0" layoutY="172.0" onAction="#checkPasswordLength" promptText="Password" />
      <Button layoutX="46.0" layoutY="326.0" mnemonicParsing="false" onAction="#SignUpOnClick" prefHeight="25.0" prefWidth="109.0" text="SignUp" />
      <TextField fx:id="usertypeTextField" editable="false" layoutX="45.0" layoutY="227.0" promptText="UserType: Client" text="Client" />
